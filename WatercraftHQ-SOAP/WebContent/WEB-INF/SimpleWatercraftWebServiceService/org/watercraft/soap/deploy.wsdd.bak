<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from SimpleWatercraftWebServiceService WSDL service -->

  <service name="SimpleWatercraftWebService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://soap.watercraft.org"/>
      <parameter name="wsdlServiceElement" value="SimpleWatercraftWebServiceService"/>
      <parameter name="schemaQualified" value="http://soap.watercraft.org"/>
      <parameter name="wsdlServicePort" value="SimpleWatercraftWebService"/>
      <parameter name="className" value="org.watercraft.soap.SimpleWatercraftWebServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="SimpleWatercraftWebService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getWatercraft" qname="operNS:getWatercraft" xmlns:operNS="http://soap.watercraft.org" returnQName="retNS:getWatercraftReturn" xmlns:retNS="http://soap.watercraft.org" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:watercraftId" xmlns:pns="http://soap.watercraft.org" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getWatercraft"/>

  </service>
</deployment>
